<h1>Listing datatables</h1>

<table>
  <tr>
    <th>Datasource</th>
    <th>Name</th>
    <th>Location</th>
    <th>Year</th>
    <th>Importdetail</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @datatables.each do |datatable| %>
  <tr>
    <td><%= datatable.datasource_id %></td>
    <td><%= datatable.name %></td>
    <td><%= datatable.location %></td>
    <td><%= datatable.year %></td>
    <td><%= datatable.importdetail %></td>
    <td><%= link_to 'Show', datatable %></td>
    <td><%= link_to 'Edit', edit_datatable_path(datatable) %></td>
    <td><%= link_to 'Destroy', datatable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Datatable', new_datatable_path %>
