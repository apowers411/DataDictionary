<%- model_class = Datatable -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:datasource_id) %>:</strong></dt>
  <dd><%= @datatable.datasource_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @datatable.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:location) %>:</strong></dt>
  <dd><%= @datatable.location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:year) %>:</strong></dt>
  <dd><%= @datatable.year %></dd>
  <dt><strong><%= model_class.human_attribute_name(:importdetail) %>:</strong></dt>
  <dd><%= @datatable.importdetail %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              datatables_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_datatable_path(@datatable), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              datatable_path(@datatable),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
